  "version": "1.0",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
   {
     "matches": ["https://www.monkey.app/*"],
     "js": ["content.js"]
   }
 ]
}